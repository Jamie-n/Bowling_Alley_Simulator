            </HBox>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Label fx:id="tabTotalLbl" alignment="CENTER" contentDisplay="CENTER" prefHeight="169.0" prefWidth="287.0" text=" Total: Â£0.00" />
